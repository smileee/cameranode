{
  "name": "cameranode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"WEB,STREAM\" -c \"bgGreen.bold,bgBlue.bold\" \"npm:dev:next\" \"npm:dev:streamer\"",
    "dev:next": "next dev -H 0.0.0.0",
    "dev:streamer": "ts-node --project tsconfig.server.json server/streamer.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pm2:start": "pm2 start npm --name \"border-camera\" -- run dev",
    "pm2:restart": "pm2 restart border-camera"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.34.0",
    "@types/mime-types": "^3.0.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "ffmpeg-static": "^5.2.0",
    "lowdb": "^7.0.1",
    "mime-types": "^3.0.1",
    "next": "^14.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
